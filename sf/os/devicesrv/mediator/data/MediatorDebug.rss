RESOURCE DEBUG_CONFIGURATION debug_conf
    {
    domain_options=ELogInclusive;
    domains={}; 
    category_options=ELogInclusive;
    categories={};
    options=KLogCommands+KLogEvents+KLogParameterData;
    output_file = "mediator.txt";
    }

